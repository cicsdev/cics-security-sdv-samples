{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
        "airbnb-base"
    ],
    "rules": {
      "no-restricted-syntax": ["off"],
      "no-unused-vars": ["error", { "varsIgnorePattern": "_ignore", "argsIgnorePattern": "^_$" }],
      "comma-dangle": ["error", "never"],
      "no-warning-comments": ["warn"]
    }
}
